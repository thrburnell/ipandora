{

  "fn": {
    "definition": "SumSq(n) = ...",
    "valid": true
  },

  "toShow": {
    "formula": "\\FORALL n in Nat. SumSq(n) = (n/6) * (n+1) * (2*n+1)",
    "valid": true
  },

  "baseCase": {
    "toShow": [
      "SumSq(0) = (0/6) * (0+1) * (2*0+1)"
    ]
  },

  "inductiveCase": {
    "arbitrary": {
      "name": "k",
      "domain": "Nat"
    },
    "hypothesis": "SumSq(k) = (k/6) * (k+1) * (2*k+1)",
    "toShow": [
      "SumSq(k+1) = ((k+1)/6) * ((k+1)+1) * (2*(k+1)+1)"
    ]
  }

}
